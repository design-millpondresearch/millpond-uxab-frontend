{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "firewall": {
    "rules": [
      {
        "name": "Block China",
        "action": "deny",
        "expression": "geo.country == 'CN'"
      }
    ]
  }
}